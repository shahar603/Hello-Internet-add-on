{
  "author": "Tim",
  "manifest_version": 2,

  "name": "Dirk from Verastablium",
  "description": "Replaces Derek from Veritasium to Dirk from Verastablium",
  "version": "1.0",
  
  "icons": { "16": "icons/Nail_and_Gear.png",
             "48": "icons/Nail_and_Gear.png",
             "128": "icons/Nail_and_Gear.png" },
  
  "browser_action": {
    "default_icon": "icons/Nail_and_Gear.png"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["switcher.js"],
      "run_at": "document_end"
    }
  ]
}